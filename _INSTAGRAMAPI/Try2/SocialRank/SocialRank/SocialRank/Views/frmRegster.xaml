<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SocialRank.Views.frmRegster">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="edtUsername" Placeholder="Email" />
            <Entry x:Name="edtPassword" Placeholder="Password" />
            <Button x:Name="btnRegister" VerticalOptions="Center" Text="Register" Clicked="btnRegister_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>