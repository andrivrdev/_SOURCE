<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SocialRankAndroid.LinkInstagramPage">
    <ContentPage.Content>
        <StackLayout Margin="20">
            <StackLayout Orientation="Horizontal">
                <Button Text="Back" HorizontalOptions="StartAndExpand" />
                <Button Text="Forward" HorizontalOptions="EndAndExpand"  />
            </StackLayout>
            <!-- WebView needs to be given height and width request within layouts to render. -->
            <WebView x:Name="webView" WidthRequest="1000" HeightRequest="1000" Navigated="webView_Navigated" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>